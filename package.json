{
  "name": "web-bands",
  "version": "1.0.0",
  "description": "Plantilla profesional en React para bandas de m√∫sica con CMS Sanity",
  "main": "index.js",
  "scripts": {
    "dev": "sanity dev",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "start:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "dev:all": "concurrently \"npm run dev\" \"npm run start:frontend\""
  },
  "keywords": [
    "react",
    "sanity",
    "cms",
    "music",
    "bands",
    "template",
    "one-page"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "@sanity/vision": "^4.1.1",
    "react": "^19.1",
    "react-dom": "^19.1",
    "sanity": "^4.1.1",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^5.0.2",
    "@types/react": "^19.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.28",
    "prettier": "^3.5",
    "typescript": "^5.8"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  }
